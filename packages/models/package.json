{
  "name": "@dumper-monorepo-example/models",
  "version": "1.0.0",
  "description": "Package of example project using mongoose-partial-dumper",
  "main": "./build/index",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\"",
    "clear": "rimraf ./build",
    "build": "yarn clear && babel src --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\" --ignore *.test.js --out-dir ./build --copy-files && tsc --emitDeclarationOnly"
  },
  "author": "Afonso Ara√∫jo Neto",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "mongoose": "^5.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/cli": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-transform-modules-commonjs": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.10",
    "@types/mongoose": "5.10.3",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "typescript": "4.2.3",
    "rimraf": "^3.0.2"
  }
}
